<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@+id/navigateToSplashFragment">

    <fragment
        android:id="@+id/navigateToSplashFragment"
        android:name="com.heady.test.modules.splash.fragments.SplashFragment"
        android:label="navigation_splash"
        tools:layout="@layout/fragment_splash" />

    <fragment
        android:id="@+id/navigateToCategoriesFragment"
        android:name="com.heady.test.modules.categories.fragments.CategoriesFragment"
        android:label="navigation_categories"
        tools:layout="@layout/fragment_categories" />

    <fragment
        android:id="@+id/navigateToSubCategoriesFragment"
        android:name="com.heady.test.modules.subcategories.fragments.SubCategoriesFragment"
        android:label="navigation_subcategories"
        tools:layout="@layout/fragment_subcategories" />

    <fragment
        android:id="@+id/navigateToProductFragment"
        android:name="com.heady.test.modules.products.fragments.ProductsFragment"
        android:label="navigation_products"
        tools:layout="@layout/fragment_products" />

</navigation>